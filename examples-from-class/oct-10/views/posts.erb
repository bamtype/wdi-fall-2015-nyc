<h1>Posts</h1>
<ul>
  <!--
    this displays all posts assigned to the @posts
    instance variable
  -->
  <% @posts.each do |post| %>
    <h2><strong>Id:</strong> <%= post.id %></h2>
    <li><strong>Body:</strong> <%= post.body %></li>
    <li><strong>User Id:</strong><%= post.user_id %></li>
  <% end %>
</ul>